<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:PhotoMover">

	<DataTemplate x:Key="ApplicationIcon">
		<Viewbox>
			<Canvas Width="160" Height="160">
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="0.4,0" EndPoint="0.6,1">
							<GradientStop Offset="0" Color="#FFFFDD7C" />
							<GradientStop Offset="1" Color="#FFFFC834" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<PathGeometry>
							<PathGeometry.Figures>
								<PathFigure IsClosed="True" StartPoint="20,90">
									<PathFigure.Segments>
										<LineSegment Point="130,90" />
										<LineSegment Point="130,160" />
										<LineSegment Point="20,160" />
									</PathFigure.Segments>
								</PathFigure>
							</PathGeometry.Figures>
						</PathGeometry>
					</Path.Data>
				</Path>
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="0.4,0" EndPoint="0.6,1">
							<GradientStop Offset="0" Color="#FFE09F00" />
							<GradientStop Offset="1" Color="#FFD79400" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<PathGeometry>
							<PathGeometry.Figures>
								<PathFigure IsClosed="True" StartPoint="20,85">
									<PathFigure.Segments>
										<LineSegment Point="50,85" />
										<LineSegment Point="60,90" />
										<LineSegment Point="50,100" />
										<LineSegment Point="20,100" />
									</PathFigure.Segments>
								</PathFigure>
							</PathGeometry.Figures>
						</PathGeometry>
					</Path.Data>
				</Path>
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="1,0" EndPoint="1,1">
							<GradientStop Offset="0" Color="#FF838383" />
							<GradientStop Offset="1" Color="#FF303030" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<GeometryGroup>
							<PathGeometry>
								<PathGeometry.Figures>
									<PathFigure IsClosed="True" StartPoint="70,30">
										<PathFigure.Segments>
											<LineSegment Point="90,30" />
											<LineSegment Point="90,120" />
											<LineSegment Point="110,120" />
											<LineSegment Point="80,150" />
											<LineSegment Point="50,120" />
											<LineSegment Point="70,120" />
										</PathFigure.Segments>
									</PathFigure>
								</PathGeometry.Figures>
							</PathGeometry>
						</GeometryGroup>
					</Path.Data>
				</Path>
				<Rectangle Canvas.Left="40" Canvas.Top="0" Width="80" Height="60">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint=".2,0" EndPoint=".8,1">
							<GradientStop Offset="0" Color="#FFB4E3FF" />
							<GradientStop Offset="1" Color="#FF1553AC" />
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<Ellipse Canvas.Left="50" Canvas.Top="10" Width="20" Height="20">
					<Ellipse.Fill>
						<RadialGradientBrush>
							<GradientStop Offset="0" Color="#FFFAFDBE" />
							<GradientStop Offset="0.7" Color="#FFFAFDBE" />
							<GradientStop Offset="1" Color="#FFFFFA00" />
						</RadialGradientBrush>
					</Ellipse.Fill>
				</Ellipse>
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="1,0" EndPoint="1,1">
							<GradientStop Offset="0" Color="#FFACACAC" />
							<GradientStop Offset="1" Color="#FF4D4D4D" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<GeometryGroup>
							<PathGeometry>
								<PathGeometry.Figures>
									<PathFigure IsClosed="True" StartPoint="60,60">
										<PathFigure.Segments>
											<LineSegment Point="100,20" />
											<LineSegment Point="120,50" />
											<LineSegment Point="120,60" />
										</PathFigure.Segments>
									</PathFigure>
								</PathGeometry.Figures>
							</PathGeometry>
						</GeometryGroup>
					</Path.Data>
				</Path>
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush StartPoint="1,0" EndPoint="1,1">
							<GradientStop Offset="0" Color="LightGray" />
							<GradientStop Offset="1" Color="#FF8D8D8D" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<GeometryGroup>
							<PathGeometry>
								<PathGeometry.Figures>
									<PathFigure IsClosed="True" StartPoint="40,60">
										<PathFigure.Segments>
											<LineSegment Point="70,30" />
											<LineSegment Point="90,60" />
										</PathFigure.Segments>
									</PathFigure>
								</PathGeometry.Figures>
							</PathGeometry>
						</GeometryGroup>
					</Path.Data>
				</Path>
			</Canvas>
		</Viewbox>
	</DataTemplate>

